{
    "name": "stonks-v3",
    "version": "1.0.0",
    "description": "An app for auto-trading cryptocurrency",
    "author": "Luke Stanbery",
    "license": "ISC",
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "build:client": "cd client && npm run build",
        "build:server": "webpack --config webpack.server.js",
        "db:create-tables": "ts-node scripts/create_db_tables.ts",
        "dev": "ts-node scripts/start_dev.ts",
        "dev:client": "cd client && npm run dev",
        "dev:server:watch": "webpack --watch --config webpack.server.js",
        "dev:server:serve": "nodemon dist/app.bundle.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prepare": "husky install",
        "start": "node dist/app.bundle.js",
        "test": "npm run test:server && npm run test:client",
        "test:client": "cd client && npm run test",
        "test:server": "jest --runInBand --detectOpenHandles"
    },
    "dependencies": {
        "@lukestanbery/ledger": "^0.1.0",
        "@popperjs/core": "^2.11.8",
        "@reduxjs/toolkit": "^1.9.7",
        "axios": "^1.6.8",
        "bootstrap": "^5.3.3",
        "express": "^4.19.2",
        "express-rate-limit": "^7.2.0",
        "mysql2": "^3.9.8",
        "node-sass": "^9.0.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.23.0",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "socket.io": "^4.7.5",
        "socket.io-client": "^4.7.5",
        "webpack-dev-middleware": "^6.1.3",
        "webpack-hot-middleware": "^2.26.1"
    },
    "devDependencies": {
        "@babel/core": "^7.24.4",
        "@babel/preset-env": "^7.24.4",
        "@babel/preset-react": "^7.24.1",
        "@babel/preset-typescript": "^7.24.1",
        "@lukestanbery/meta-importer": "^1.0.1",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.13",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^14.3.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.7",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.0",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "autoprefixer": "^10.4.19",
        "chalk": "^5.3.0",
        "concurrently": "^8.2.2",
        "css-loader": "^6.11.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-fetch-mock": "^3.0.3",
        "nodemon": "^3.1.0",
        "postcss-loader": "^7.3.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-refresh": "^0.14.2",
        "sass": "^1.75.0",
        "sass-loader": "^13.3.3",
        "style-loader": "^3.3.4",
        "supertest": "^6.3.4",
        "terser-webpack-plugin": "^5.3.10",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.2",
        "webpack-node-externals": "^3.0.0"
    }
}
