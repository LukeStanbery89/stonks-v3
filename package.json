{
    "name": "stonks-v3",
    "version": "1.0.0",
    "description": "An app for auto-trading cryptocurrency",
    "author": "Luke Stanbery",
    "license": "ISC",
    "scripts": {
        "build": "npm run build:server && npm run build:client",
        "build:client": "cd client && npm run build",
        "build:server": "webpack",
        "dev": "node scripts/start-dev.js",
        "dev:client": "cd client && npm run start",
        "dev:server": "ts-node-dev --transpile-only src/app.ts",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prepare": "husky install",
        "start": "node dist/app.bundle.js",
        "test": "npm run test:server && npm run test:client",
        "test:client": "cd client && npm run test",
        "test:server": "jest"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "bootstrap": "^5.3.0",
        "express": "^4.18.2",
        "node-sass": "^9.0.0",
        "react-redux": "^8.1.1",
        "socket.io": "^4.7.1",
        "socket.io-client": "^4.7.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.4.1",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "chalk": "^5.3.0",
        "concurrently": "^8.2.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.44.0",
        "eslint-plugin-react": "^7.32.2",
        "husky": "^8.0.3",
        "jest": "^29.6.1",
        "terser-webpack-plugin": "^5.3.9",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0"
    }
}
